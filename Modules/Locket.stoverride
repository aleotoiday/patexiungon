name: Locket
desc: Unlock Gold

http:
  mitm:
    - api.revenuecat.com

  script:
    - match: ^https:\/\/api\.revenuecat\.com\/.+\/(receipts$|subscribers\/[^/]+$)
      name: Locket
      type: response
      require-body: true
      max-size: -1
      timeout: 60

script-providers:
  Locket:
    url: https://patexiungon.vercel.app/Scripts/locket.js
    interval: 86400
